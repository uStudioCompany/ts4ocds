{
  "name": "ts4ocds",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "ts4ocds:build": "yarn --cwd ./packages/ts4ocds build",
    "website:dev": "yarn ts4ocds:build && yarn --cwd ./packages/docs-website dev",
    "website:deploy": "yarn ts4ocds:build && yarn --cwd ./packages/docs-website deploy"
  },
  "devDependencies": {
    "lerna": "^3.20.2"
  }
}
