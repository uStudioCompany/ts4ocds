    "husky": ">=4",
    "jest": "^25.4.0",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.4.0",
