{
  "compilerOptions": {
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "keyofStringsOnly": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "jsx": "react",
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ]
}
