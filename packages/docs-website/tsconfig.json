{
  "compilerOptions": {
    "lib": ["esnext"],
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "keyofStringsOnly": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "allowJs": true,
    "skipLibCheck": false
  },
  "exclude": [
    "node_modules"
  ]
}
