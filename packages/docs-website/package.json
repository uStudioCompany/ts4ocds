{
  "name": "ts4ocds-website",
  "version": "1.0.0",
  "scripts": {
    "clean": "gatsby clean",
    "copy-readme": "copyfiles -u 2 ../../README.md .",
    "build": "yarn clean && yarn copy-readme && gatsby build --prefix-paths",
    "deploy": "yarn build && gh-pages -d public",
    "dev": "yarn clean && yarn copy-readme && gatsby develop"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "copyfiles": "^2.2.0",
    "gatsby": "^2.19.45",
    "gatsby-image": "^2.2.44",
    "gatsby-plugin-manifest": "^2.2.48",
    "gatsby-plugin-offline": "^3.0.41",
    "gatsby-plugin-react-helmet": "^3.1.24",
    "gatsby-plugin-sharp": "^2.4.13",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-source-filesystem": "^2.1.56",
    "gatsby-transformer-sharp": "^2.3.19",
    "gh-pages": "^2.2.0",
    "raw-loader": "^4.0.0",
    "@reach/router": "^1.3.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.0.1",
    "ustudio-ui": "^1.0.1",
    "react-syntax-highlighter": "^12.2.1",
    "react-markdown": "^4.3.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/uuid": "^7.0.2",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/react-helmet": "^5.0.15",
    "@types/styled-components": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "husky": ">=4",
    "lint-staged": ">=10",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "prettier": "^2.0.2",
    "typescript": "^3.8.3",
    "ts-node": "^8.8.1"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx?": "prettier --write"
  }
}
